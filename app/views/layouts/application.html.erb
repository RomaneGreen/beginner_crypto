<!DOCTYPE html>
<html>
  <head>
    <title>Crypto Currency Portfolio</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%= favicon_link_tag asset_path('ctech.jpg'), :rel => 'icon', :type => 'image/jpg' %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <%=render 'home/navbar'%>

    <%=render 'home/flash'%>
    <%= yield %>

  </body>
</html>