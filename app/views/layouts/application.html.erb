<!DOCTYPE html>
<html>
  <head>
    <title>Knew 2 Crypto</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%= favicon_link_tag asset_path('knewtocrypto.png'), :rel => 'icon', :type=> 'image/png' %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <%=render 'home/navbar'%>
    <div class="container">
      <%=render 'home/flash'%>
      <%= yield %>
    </div>

  </body>
</html>